[Unit]
Description=Univa Grid Engine Execd
After=network.target remote-fs.target

[Service]
Type=forking
KillMode=process
ExecStart=/bin/sh <%= @gridengineroot %>/<%= @gridenginecell %>/common/sgeexecd start
ExecStop=/bin/sh <%= @gridengineroot %>/<%= @gridenginecell %>/common/sgeexecd stop

[Install]
WantedBy=multi-user.target